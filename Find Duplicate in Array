#include <bits/stdc++.h>

int findDuplicate(vector<int> &arr, int n){
	// Write your code here.
	sort(arr.begin(),arr.end());
    int i =0,j=i+1;
    while(j<n){
        if(arr[i]==arr[j]){
            return arr[i];
            break;
        }
        else{
            i++;
            j++;
        }
    }
}
